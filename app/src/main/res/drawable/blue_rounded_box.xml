<!-- edittext_border.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#F0F5FF" /> <!-- 테두리 내부를 투명하게 설정 -->
    <stroke
        android:width="1dp"
        android:color="#0062FE"/> <!-- 테두리 색상을 조절 -->
    <corners android:radius="16dp" /> <!-- 테두리의 모서리를 둥글게 조절 -->
</shape>
